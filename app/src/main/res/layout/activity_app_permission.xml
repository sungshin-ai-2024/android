<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="16dp"
    tools:context=".MainActivity">
    <ImageButton
        android:id="@+id/left"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="20dp"
        android:background="@drawable/img_left"
        app:layout_constraintRight_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:contentDescription="뒤로 가기"/>
    <TextView
        android:id="@+id/txt_setting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:fontFamily="@font/suit_bold"
        android:text="이 앱이 사용하는 권한"
        android:textColor="@color/font"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <View
        android:id="@+id/v1_main"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="20dp"
        android:background="@color/gray4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/left"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/con_layout"
        android:layout_width="match_parent"
        android:layout_height="621dp"
        android:layout_marginHorizontal="20dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@id/v1_main">
        <TextView
            android:id="@+id/txt_acct1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="top"
            android:fontFamily="@font/suit_bold"
            android:text="세이브위드 접근 허용"
            android:textColor="@color/font"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="@id/con_layout"
            app:layout_constraintTop_toTopOf="@id/con_layout"/>

        <LinearLayout
            android:id="@+id/linear1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@id/txt_acct1"
            app:layout_constraintStart_toStartOf="@id/con_layout"
            app:layout_constraintEnd_toEndOf="@id/con_layout"
            android:layout_weight="1">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/sett_box4"
                android:layout_width="0dp"
                android:layout_height="230dp"
                android:background="@drawable/box_gray5"
                android:layout_weight="1">
                <TextView
                    android:id="@+id/sett_txt_logout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:fontFamily="@font/suit_medium"
                    android:text="위치 서비스"
                    android:textColor="@color/font"
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@id/sett_box4"/>
                <Switch
                    android:id="@+id/swch_sensitive_info"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    app:layout_constraintTop_toTopOf="@id/sett_txt_logout"
                    app:layout_constraintBottom_toBottomOf="@id/sett_txt_logout"
                    app:layout_constraintEnd_toEndOf="@id/sett4_l1"/>
                <View
                    android:id="@+id/sett4_l1"
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:layout_marginTop="12dp"
                    android:background="@color/gray3"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/sett_txt_logout" />

                <TextView
                    android:id="@+id/sett_photo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="12dp"
                    android:fontFamily="@font/suit_medium"
                    android:text="사진"
                    android:textColor="@color/font"
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@id/sett4_l1"/>
                <Switch
                    android:id="@+id/swch_photo"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    app:layout_constraintTop_toTopOf="@id/sett_photo"
                    app:layout_constraintBottom_toBottomOf="@id/sett_photo"
                    app:layout_constraintEnd_toEndOf="@id/sett4_l2"/>
                <View
                    android:id="@+id/sett4_l2"
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:layout_marginTop="12dp"
                    android:background="@color/gray3"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/sett_photo" />

                <TextView
                    android:id="@+id/sett_notif"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="12dp"
                    android:fontFamily="@font/suit_medium"
                    android:text="알림"
                    android:textColor="@color/font"
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@id/sett4_l2"/>
                <Switch
                    android:id="@+id/swch_notif"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    app:layout_constraintTop_toTopOf="@id/sett_notif"
                    app:layout_constraintBottom_toBottomOf="@id/sett_notif"
                    app:layout_constraintEnd_toEndOf="@id/sett4_l3"/>
                <View
                    android:id="@+id/sett4_l3"
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:layout_marginTop="12dp"
                    android:background="@color/gray3"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/sett_notif" />

                <TextView
                    android:id="@+id/sett_phone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="12dp"
                    android:fontFamily="@font/suit_medium"
                    android:text="전화"
                    android:textColor="@color/font"
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@id/sett4_l3"/>
                <Switch
                    android:id="@+id/swch_phone"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    app:layout_constraintTop_toTopOf="@id/sett_phone"
                    app:layout_constraintBottom_toBottomOf="@id/sett_phone"
                    app:layout_constraintEnd_toEndOf="@id/sett4_l4"/>
                <View
                    android:id="@+id/sett4_l4"
                    android:layout_width="wrap_content"
                    android:layout_height="1dp"
                    android:layout_marginTop="12dp"
                    android:background="@color/gray3"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/sett_phone" />

                <TextView
                    android:id="@+id/sett_healthapp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="12dp"
                    android:fontFamily="@font/suit_medium"
                    android:text="헬스앱"
                    android:textColor="@color/font"
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@id/sett4_l4"/>
                <Switch
                    android:id="@+id/swch_healthapp"
                    android:layout_width="48dp"
                    android:layout_height="48dp"
                    app:layout_constraintTop_toTopOf="@id/sett_healthapp"
                    app:layout_constraintBottom_toBottomOf="@id/sett_healthapp"
                    app:layout_constraintEnd_toEndOf="@id/sett4_l4"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>