<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="13dp"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/login1_logo"
        android:layout_width="230dp"
        android:layout_height="237.33dp"
        android:layout_marginTop="160dp"
        android:src="@drawable/img_logo_login1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/login1_btn_yellow"
        android:layout_width="335dp"
        android:layout_height="50dp"
        android:layout_marginTop="495dp"
        android:background="@drawable/box_yellow"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:contentDescription="카카오 로그인 버튼"/>

    <ImageButton
        android:id="@+id/login1_btn_gray3"
        android:layout_width="335dp"
        android:layout_height="50dp"
        android:layout_marginTop="557dp"
        android:background="@drawable/box_gray3_bttn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:contentDescription="구글 로그인 버튼"/>

</androidx.constraintlayout.widget.ConstraintLayout>